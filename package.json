        "axios": "^0.19",
        "babel-jest": "^24.9.0",
        "babel-plugin-transform-export-extensions": "^6.22.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "cross-env": "^5.1",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16.2": "^1.6.1",
        "jest": "^24.9.0",
        "jest-cli": "^24.9.0",
        "jest-enzyme": "^7.1.1",
        "jquery": "^3.5",
        "laravel-mix": "^4.0.7",
